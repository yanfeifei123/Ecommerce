.cates {
  display: flex;
  align-items: flex-start;
  justify-content: center;
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.logoview {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 15px;
  flex-wrap: wrap;
}

.logoimage {
  width: 75px;
  height: 75px;
  border-radius: 50%;
}

.content {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  flex-wrap: wrap;
}

.error {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 80%;
  padding-left: 15rpx;
  padding-right: 15rpx;
  font-size: 30rpx;
  color: var(--themeColor);
}

.textview {
  display: flex;
  align-items: center;
  justify-content: start;
  width: 90%;
  border-bottom: 1px solid #ededed;
  padding-left: 15rpx;
  padding-right: 15rpx;
  height: 50px;
}

.textview .text {
  width: 20%;
  display: flex;
  align-items: center;
  justify-content: start;
  font-weight: bold;
  font-size: 28rpx;
}

.lg {
  width: 100%;
  font-size: 25px;
}

.textview .inputtext {
  width: 80%;
}

.button {
  width: 90%;
  background-color: #FEDD4B;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: 600px;
}